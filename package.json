{
  "name": "delphi-fabric",
  "private": true,
  "description": "Focusing on study fabric capability",
  "keywords": [
    "fabric-sdk-node",
    "sample"
  ],
  "scripts": {
    "install": "cd common/nodejs; npm install; lerna bootstrap --no-ci; cd -",
    "nuke": "cd common/nodejs; rm -rf node_modules; lerna clean --yes; cd -; rm -rf node_modules;",
    "test": "mocha test",
    "prestart": "mocha dockerode-bootstrap.js --grep ^up --bail",
    "start": "mocha app/channelSetup.js",
    "poststart": "mocha ./cc/golang/diagnose/install.js",
    "nodejscc": "mocha ./cc/node/diagnose/install.js",
    "stop": "mocha dockerode-bootstrap.js --grep ^down"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "khala-axios": "latest",
    "@davidkhala/dockerode": "latest",
    "@davidkhala/light": "latest",
    "@davidkhala/nodeutils": "latest"
  },
  "devDependencies": {
    "lerna": "latest",
    "mocha": "latest",
    "@davidkhala/logger": "latest"
  },
  "type": "module"
}
