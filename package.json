{
  "name": "delphi-fabric",
  "private": true,
  "description": "Focusing on study fabric capability",
  "keywords": [
    "fabric-sdk-node",
    "sample"
  ],
  "scripts": {
    "install": "cd common/nodejs; npm install; lerna bootstrap --no-ci; cd -",
    "nuke": "cd common/nodejs; rm -rf node_modules; lerna clean --yes; cd -; rm -rf node_modules;",
    "test": "mocha test",
    "prestart": "./docker.sh prepareNetwork",
    "start": "binPath=$PWD/common/bin channelName=allchannel mocha app/channelSetup.js --grep \"^channelSetup\"",
    "poststart": "./docker.sh cc",
    "stop": "./docker.sh down",
    "channelSetup": "mocha app/channelSetup.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "khala-axios": "latest",
    "@davidkhala/dockerode": "latest",
    "@davidkhala/light": "latest",
    "@davidkhala/nodeutils": "latest"
  },
  "devDependencies": {
    "lerna": "latest",
    "mocha": "latest",
    "@davidkhala/logger": "latest"
  },
  "type": "module"
}
