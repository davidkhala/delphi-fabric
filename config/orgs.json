{
  "domain": "Delphi.com",
  "TLS": false,
  "docker": {
    "fabricTag": "1.1.0-preview",
    "network": "delphiNetwork",
    "volumes": {
      "CONFIGTX": {
        "local": "CONFIGTX_local",
        "swarm": "CONFIGTX_swarm",
        "dir": "/home/davidliu/Documents/delphi-fabric/config/configtx"
      },
      "MSPROOT": {
        "local": "MSPROOT_local",
        "swarm": "MSPROOT_swarm",
        "dir": "/home/davidliu/Documents/delphi-fabric/config/crypto-config/"
      }
    }
  },
  "orderer": {
    "type": "kafka",
    "genesis_block": {
      "file": "delphi.block",
      "profile": "delphiGenesis"
    },
    "solo": {
      "container_name": "orderContainerName",
      "portMap": {
        "7050": 7050
      }
    },
    "kafka": {
      "M": 2,
      "N": 3,
      "rule": "1<M<N<K,K>=4,Orderer.AbsoluteMaxBytes < replica.fetch.max.bytes <= message.max.bytes",
      "zookeepers": {
        "zookeeper0": {
          "MY_ID": 0
        },
        "zookeeper1": {
          "MY_ID": 1
        },
        "zookeeper2": {
          "MY_ID": 2
        }
      },
      "kafkas": {
        "kafka0": {
          "BROKER_ID": 0
        },
        "kafka1": {
          "BROKER_ID": 1
        },
        "kafka2": {
          "BROKER_ID": 2
        },
        "kafka3": {
          "BROKER_ID": 3
        }
      },
      "orderers": {
        "orderer0": {
          "portMap": {
            "7050": 7050
          }
        },
        "orderer1": {
          "portMap": {
            "7050": 8050
          }
        },
        "orderer2": {
          "portMap": {
            "7050": 9050
          }
        }
      }
    },
    "MSP": {
      "name": "OrdererMSPName",
      "id": "OrdererMSP"
    }
  },
  "channels": {
    "delphiChannel": {
      "file": "delphi.tx",
      "eventWaitTime": 30000,
      "orgs": {
        "BU": {
          "peerIndexes": [
            0,
            1
          ]
        },
        "PM": {
          "peerIndexes": [
            0
          ]
        },
        "ENG": {
          "peerIndexes": [
            0
          ]
        }
      }
    },
    "BUChannel": {
      "eventWaitTime": 30000,
      "file": "bu.tx",
      "orgs": {
        "BU": {
          "peerIndexes": [
            0,
            1
          ]
        }
      }
    }
  },
  "orgs": {
    "BU": {
      "peers": [
        {
          "swarm": {
            "constraints": [
              "node.hostname == ubuntu"
            ]
          },
          "container_name": "BUContainerName",
          "portMap": [
            {
              "host": 7051,
              "container": 7051
            },
            {
              "host": 7053,
              "container": 7053
            }
          ]
        },
        {
          "swarm": {
            "constraints": [
              "node.hostname == ubuntu"
            ]
          },
          "container_name": "BUContainer1",
          "portMap": [
            {
              "host": 7061,
              "container": 7051
            },
            {
              "host": 7063,
              "container": 7053
            }
          ]
        }
      ],
      "userCount": 0,
      "ca": {
        "enable": true,
        "admin": {
          "name": "CAadmin",
          "pass": "passwd"
        },
        "portHost": 7054,
        "tlsca": {
          "portHost": 7055
        }
      },
      "MSP": {
        "name": "BUMSPName",
        "id": "BUMSP"
      }
    },
    "ENG": {
      "peers": [
        {
          "swarm": {
            "constraints": [
              "node.hostname == fabric-swarm-manager"
            ]
          },
          "container_name": "ENGContainerName",
          "portMap": [
            {
              "host": 8051,
              "container": 7051
            },
            {
              "host": 8053,
              "container": 7053
            }
          ]
        }
      ],
      "userCount": 0,
      "ca": {
        "enable": false,
        "admin": {
          "name": "CAadmin",
          "pass": "passwd"
        },
        "portHost": 8054,
        "tlsca": {
          "portHost": 8055
        }
      },
      "MSP": {
        "name": "ENGMSPName",
        "id": "ENGMSP"
      }
    },
    "PM": {
      "peers": [
        {
          "swarm": {
            "constraints": [
              "node.hostname == fabric-swarm-manager"
            ]
          },
          "container_name": "PMContainerName",
          "portMap": [
            {
              "host": 9051,
              "container": 7051
            },
            {
              "host": 9053,
              "container": 7053
            }
          ]
        }
      ],
      "userCount": 0,
      "ca": {
        "enable": false,
        "admin": {
          "name": "CAadmin",
          "pass": "passwd"
        },
        "portHost": 9054,
        "tlsca": {
          "portHost": 9055
        }
      },
      "MSP": {
        "name": "PMMSPName",
        "id": "PMMSP"
      }
    }
  }
}
