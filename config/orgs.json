{
  "TLS": true,
  "docker": {
    "fabricTag": "1.4.6",
    "caTag": "1.4.6",
    "thirdPartyTag": "0.4.18",
    "network": "delphiNetwork",
    "volumes": {
      "CONFIGTX": "Documents/delphi-fabric/config/configtx",
      "MSPROOT": "Documents/delphi-fabric/config/ca-crypto-config/"
    }
  },
  "orderer": {
    "type": "etcdraft",
    "genesis_block": {
      "file": "delphi.block",
      "profile": "delphiGenesis"
    },
    "kafka": {
      "M": 2,
      "N": 3,
      "zookeepers": {
        "zookeeper0": {
          "MY_ID": 0
        },
        "zookeeper1": {
          "MY_ID": 1
        },
        "zookeeper2": {
          "MY_ID": 2
        }
      },
      "kafkas": {
        "kafka0": {
          "BROKER_ID": 0
        },
        "kafka1": {
          "BROKER_ID": 1
        },
        "kafka2": {
          "BROKER_ID": 2
        },
        "kafka3": {
          "BROKER_ID": 3
        }
      },
      "organizations": {
        "hyperledger": {
          "mspid": "hyperledgerMSP",
          "ca": {
            "portHost": 7454
          },
          "orderers": {
            "orderer0": {
              "portHost": 7050
            },
            "orderer1": {
              "portHost": 8050
            },
            "orderer2": {
              "portHost": 9050
            }
          }
        }
      }
    },
    "etcdraft": {
      "organizations": {
        "hyperledger": {
          "mspid": "hyperledgerMSP",
          "ca": {
            "portHost": 7454
          },
          "intermediateCA": {
            "portHost": 7455
          },
          "orderers": {
            "orderer0": {
              "portHost": 7050,
              "operations": {
                "port": 8443
              }
            }
          }
        }
      }
    }
  },
  "channels": {
    "allchannel": {
      "file": "all.tx",
      "organizations": {
        "astri.org": {
          "peerIndexes": [
            0,
            1
          ],
          "anchorPeerIndexes": [
            0,
            1
          ]
        },
        "icdd": {
          "peerIndexes": [
            0,
            1
          ],
          "anchorPeerIndexes": [
            0,
            1
          ]
        }
      }
    },
    "extrachannel": {
      "file": "extra.tx",
      "organizations": {
        "astri.org": {
          "peerIndexes": [
            0
          ],
          "anchorPeerIndexes": [
            0
          ]
        },
        "icdd": {
          "peerIndexes": [
            0
          ],
          "anchorPeerIndexes": [
            0
          ]
        }
      }
    }
  },
  "organizations": {
    "astri.org": {
      "peers": [
        {
          "container_name": "peer0.astri.org",
          "port": 7051,
          "operations": {
            "port": 9443
          },
          "metrics": {
            "provider": null,
            "statsD": {
              "host": "statsd-exporter"
            }
          },
          "couchDB": {
            "container_name": "couchdb.peer0.astri.org",
            "port": 7984
          }
        },
        {
          "container_name": "peer1.astri.org",
          "port": 7151,
          "metrics": {
            "provider": null,
            "statsD": {
              "host": "statsd-exporter"
            }
          }
        },
        {}
      ],
      "ca": {
        "portHost": 7054
      },
      "mspid": "ASTRIMSP"
    },
    "icdd": {
      "peers": [
        {
          "container_name": "peer0.icdd",
          "port": 8051
        },
        {
          "container_name": "peer1.icdd",
          "port": 8151
        }
      ],
      "ca": {
        "portHost": 8054
      },
      "mspid": "icddMSP"
    }
  }
}
